{
  "name": "Twitch Utilities",
  "version": "0.0.1",
  "description": "A lightweight collection of small Twitch utilities",
  "manifest_version": 2,
  "permissions": [
    "notifications"
  ],
  "content_scripts": [{
    "matches": ["https://www.twitch.tv/*"],
    "js": ["inject.js"],
    "css": ["inject.css"]
  }]
}
